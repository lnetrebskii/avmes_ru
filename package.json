{
  "name": "avmes_ru",
  "version": "1.0.0",
  "description": "Build tooling for Tailwind CSS assets",
  "scripts": {
    "build:css": "tailwindcss -c tailwind.config.js -i ./assets/css/style.css -o ./assets/css/dist-style.css --minify",
    "watch:css": "tailwindcss -c tailwind.config.js -i ./assets/css/style.css -o ./assets/css/dist-style.css --watch --minify"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "autoprefixer": "^10.4.22",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.14"
  }
}
